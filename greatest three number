#include<iostream>
using namespace std;
int main(){
    int a;
    int b;
    int c;
    cin>>a>>b>>c;
    if(a>b){
        if(a>c){
            cout<<"a is greater than b and c"<<endl;
        }
        else{
            if (c>b){
                cout<<"c is greater than all"<<endl;
            }
            else{
                cout<<"b is greater"<<endl;
            }
        }
    }
     else {
            if (b>c){
                cout<<"b is greater"<<endl;
            }
        else{
            cout<<"c is greater"<<endl;
        }
     }
        return 0;
    }
